# Vite Performance Optimizations
# Add these to your .env file for better performance

# Enable SWC (faster than Babel)
VITE_USE_SWC=true

# Disable unnecessary features in development
VITE_DEV_SOURCE_MAP=false

# Optimize chunk size
VITE_CHUNK_SIZE_WARNING_LIMIT=5120

# Enable build cache
VITE_CACHE_DIR=node_modules/.vite

